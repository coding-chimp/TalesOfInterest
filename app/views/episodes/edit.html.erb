<% present @episode do |episode_presenter| %>
<% title "Edit #{@episode.podcast.name} #{episode_presenter.num}: #{@episode.title}" %>

<div class="large-8 large-offset-1">
  <h2>Edit Episode</h2>
</div>

<%= simple_form_for [@episode.podcast, @episode], url: { :action => :update } do |f| %>
	<%= render 'form', f: f %>
<% end %>

<% end %>