<div id="<%= podcast.name %>SubscribersModal" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3><%= podcast.name %> Subscribers</h3>
  </div>
  <div class="modal-body">
    <%= content_tag :div, "", id: "#{podcast.name}_subscribers_chart", style: "height: 400px; width: 530px", data: {subscribers: subscribers_chart_data(podcast.name)} %>
  </div>
</div>
